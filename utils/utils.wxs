function funcToCN(index) {
  var arr = ['一', '二', '三', '四', '五'];
  return arr[index];
}

function weatherCNtoStr(cn) {
  var obj = {
    '晴朗': 'skies',
    '暴雨': 'showers',
    '小雪': 'snow',
    '强风': 'Gales',
    '薄雾': 'Fog',
    '热浪': 'blistering',
    '暴雷': 'Thunder',
    '打雷': 'Thunder',
    '暴雪': 'blizzardy',
    '灵风': 'UmbralWind',
    '妖雾': 'Gloom',
    '雷雨': 'Thunderstorms'
  }
  return obj[cn] || '';
}

function timeClass(str) {
  var obj = {
    '00:00': 'night',
    '08:00': 'mornning',
    '16:00': 'dusk'
  }
  return obj[str] || '';
}

function keepTime(name, value) {
  if (name.indexOf('记忆') > -1 && value === null) {
    return '永久';
  } else if (value !== null) {
    return value;
  } else {
    return '-';
  }
}

module.exports = {
  funcToCN: funcToCN,
  weatherCNtoStr: weatherCNtoStr,
  timeClass: timeClass,
  keepTime: keepTime
}